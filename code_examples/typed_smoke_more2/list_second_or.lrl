(def second_or (pi xs ListRc (pi d Nat Nat))
  (lam xs ListRc
    (lam d Nat
      (match xs Nat
        (case (nil_rc) d)
        (case (cons_rc h t ih)
          (match t Nat
            (case (nil_rc) h)
            (case (cons_rc h2 t2 ih2) h2)))))))

(second_or (cons_rc zero (cons_rc (succ zero) nil_rc)) (succ (succ zero)))
