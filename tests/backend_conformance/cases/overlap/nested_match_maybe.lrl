;;! tag: overlap-subset
;;! expect_exit: 0
;;! expect_result_kind: nat
;;! expect_result: 2

(inductive copy MaybeNat (sort 1)
  (ctor none MaybeNat)
  (ctor some (pi n Nat MaybeNat)))

(def unwrap_twice (pi m MaybeNat Nat)
  (lam m MaybeNat
    (match m Nat
      (case (none) zero)
      (case (some x)
        (match x Nat
          (case (zero) zero)
          (case (succ y) (succ y)))))))

(def entry Nat
  (unwrap_twice (some (succ (succ zero)))))
