;; Edge Case 5c: Factorial 3 = 6

(def mul (pi a Nat (pi b Nat Nat))
  (lam a Nat
    (lam b Nat
      (match a Nat
        (case (zero) zero)
        (case (succ m ih) (add b ih))))))

(def factorial (pi n Nat Nat)
  (lam n Nat
    (match n Nat
      (case (zero) (succ zero))
      (case (succ m ih) (mul n ih)))))

;; factorial 3 = 6
(factorial (succ (succ (succ zero))))
