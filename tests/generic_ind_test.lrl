(inductive MyBox Type
  (ctor mkBox (pi n Nat MyBox)))

(def getBox (pi b MyBox Nat)
  (lam b MyBox
    (match b Nat
      (case (mkBox n) n)
    )
  )
)

(def res1 Nat (getBox (mkBox (succ zero))))

(inductive MyNat Type
  (ctor myzero MyNat)
  (ctor mysucc (pi n MyNat MyNat)))

(def toNat (pi m MyNat Nat)
  (lam m MyNat
    (match m Nat
      (case (myzero) zero)
      (case (mysucc n ih) (succ ih))
    )
  )
)

(def res2 Nat (toNat (mysucc (mysucc myzero))))
res1
res2
