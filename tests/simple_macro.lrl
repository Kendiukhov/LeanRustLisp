
(def q1 (List Nat) (quote (1)))
q1

(defmacro mk_singleton (val)
  (quasiquote (cons (unquote val) nil)))

(def q2 (List Nat) (mk_singleton 1))
q2

(def x Nat 10)
(def q3 (List Nat) (mk_singleton x))
q3

;; (quote x) inside macro should substitute x if substitution descends
(defmacro basic-quote (val) 
  (quote val))

(basic-quote 42)
