;; stdlib bool.or: full truth table.
;; expected true-count = 3

(def bool_to_nat (pi b Bool Nat)
  (lam b Bool
    (if_nat b (succ zero) zero)))

(def entry Nat
  (add
    (add
      (bool_to_nat (or true true))
      (bool_to_nat (or true false)))
    (add
      (bool_to_nat (or false true))
      (bool_to_nat (or false false)))))
