;; stdlib nat_le representative cases.
;; expected true-count = 2

(def bool_to_nat (pi b Bool Nat)
  (lam b Bool
    (if_nat b (succ zero) zero)))

(def one Nat (succ zero))
(def two Nat (succ one))
(def three Nat (succ two))

(def entry Nat
  (add
    (add
      (bool_to_nat (nat_le one three))
      (bool_to_nat (nat_le two two)))
    (bool_to_nat (nat_le three one))))
