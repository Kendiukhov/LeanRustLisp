;; stdlib nat_lt representative cases.
;; expected true-count = 1

(def bool_to_nat (pi b Bool Nat)
  (lam b Bool
    (if_nat b (succ zero) zero)))

(def one Nat (succ zero))
(def two Nat (succ one))
(def three Nat (succ two))

(def entry Nat
  (add
    (add
      (bool_to_nat (nat_lt one three))
      (bool_to_nat (nat_lt two two)))
    (bool_to_nat (nat_lt three one))))
