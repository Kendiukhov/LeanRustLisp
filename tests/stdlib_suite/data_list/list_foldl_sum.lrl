;; foldl sum over [1,2,3,4] => 10.
;; expected: entry = 10

(def plus_step (pi acc Nat (pi x Nat Nat))
  (lam acc Nat
    (lam x Nat
      (add acc x))))

(def xs (List Nat)
  (cons (succ zero)
    (cons (succ (succ zero))
      (cons (succ (succ (succ zero)))
        (cons (succ (succ (succ (succ zero)))) nil)))))

(def entry Nat
  (foldl plus_step zero xs))
