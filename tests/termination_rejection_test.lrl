;; Termination smoke for current contracts (total defs only).

(def is_zero_local (pi n Nat Bool)
  (lam n Nat
    (match n Bool
      (case (zero) true)
      (case (succ m ih) false))))

(def pred_local (pi n Nat Nat)
  (lam n Nat
    (match n Nat
      (case (zero) zero)
      (case (succ m ih) m))))

(is_zero_local (pred_local (succ (succ zero))))
